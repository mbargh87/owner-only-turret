{
  "name": "owner-only-turret-contracts",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "mud build",
    "clean": "forge clean && rm -rf src/**/codegen",
    "deploy:local": ". ./.env && pnpm run build && mud deploy --profile=local --worldAddress ${WORLD_ADDRESS:-$1}",
    "deploy:pyrope": ". ./.env.pyrope && pnpm run build && mud deploy --profile=pyrope --worldAddress ${WORLD_ADDRESS:-$1}",
    "test": ". ./.env && forge test --fork-url $RPC_URL",
    "prettier": "prettier --write 'src/**/*.sol'",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol' --fix"
  },
  "dependencies": {
    "@eveworld/smart-object-framework-v2": "0.1.2",
    "@eveworld/world-v2": "0.1.2",
    "@eveworld/common-constants": "0.0.13",
    "@latticexyz/cli": "2.2.22-7367a813d9b394415ba70678c4295cc90562c304",
    "@latticexyz/schema-type": "2.2.22-7367a813d9b394415ba70678c4295cc90562c304",
    "@latticexyz/store": "2.2.22-7367a813d9b394415ba70678c4295cc90562c304",
    "@latticexyz/world": "2.2.22-7367a813d9b394415ba70678c4295cc90562c304",
    "@latticexyz/world-modules": "2.2.22-7367a813d9b394415ba70678c4295cc90562c304"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "ds-test": "https://github.com/dapphub/ds-test.git#e282159d5170298eb2455a6c05280ab5a73a4ef0",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#74cfb77e308dd188d2f58864aaf44963ae6b88b1",
    "prettier": "3.2.5",
    "prettier-plugin-solidity": "1.3.1",
    "solhint": "^3.3.7"
  }
}